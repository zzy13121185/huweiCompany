<view><cu-custom vue-id="0bd2d447-1" bgColor="bg-olive" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">消防检查</view></cu-custom><view style="display:flex;flex-direction:column;width:100%;height:100vh;position:relative;" id="row"><view class="flex solid-bottom justify-around bg-white" style="height:100rpx;position:absolute;top:0px;width:100%;"><view class="bg-white padding"><text class="cuIcon-title text-orange"></text>{{"待检查:"+overDue}}</view><view class="bg-white padding"><text class="cuIcon-title text-green"></text>{{"已检查:"+finish}}</view></view><view class="cu-form-group" style="border-top:1rpx solid #eee;margin-top:101rpx;"><view class="title">选择轮次</view><picker value="{{index}}" range="{{picker}}" data-event-opts="{{[['change',[['PickerChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{index>-1?picker[index]:'请选择轮次'}}</view></picker></view><view class="padding" style="flex:1;"><block wx:if="{{showBuilding}}"><view class="cu-form-group"><view class="title" style="width:200rpx;">地点</view><view style="width:120rpx;"><text class="cuIcon-title text-yellow"></text>待检查</view><view style="width:120rpx;"><text class="cuIcon-title text-green"></text>已检查</view><view style="width:90rpx;"><text class="cuIcon-title text-red"></text>隐患</view></view></block><block wx:for="{{placeArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{showBuilding}}"><view data-event-opts="{{[['tap',[['next',['$0'],[[['placeArr','',index]]]]]]]}}" class="cu-form-group" bindtap="__e"><view class="title" style="width:200rpx;">{{item.Name}}</view><view style="width:120rpx;text-align:center;color:#fbbd08;">{{''+item.overdue+''}}</view><view style="width:120rpx;text-align:center;color:#39b54a;">{{''+item.finish+''}}</view><view style="width:90rpx;text-align:center;color:#e54d42;">{{''+item.danger+''}}</view></view></block></block><block wx:for="{{placeArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{!showBuilding}}"><view data-event-opts="{{[['tap',[['gotoDetail',['$0'],[[['placeArr','',index]]]]]]]}}" class="basis-sm bg-white margin-xs padding-sm radius" bindtap="__e"><view>{{item.Name}}</view><view>状态：<block wx:if="{{!item.isSafe}}"><text class="cuIcon-title text-yellow"></text></block><block wx:if="{{!item.isSafe}}"><text>待检查</text></block><block wx:if="{{item.isSafe=='True'}}"><text class="cuIcon-title text-green"></text></block><block wx:if="{{item.isSafe=='True'}}"><text>安全</text></block><block wx:if="{{item.isSafe=='False'}}"><text class="cuIcon-title text-red"></text></block><block wx:if="{{item.isSafe=='False'}}"><text>隐患</text></block></view></view></block></block></view><view class="padding"><block wx:if="{{showBack}}"><button data-event-opts="{{[['tap',[['reset',['$event']]]]]}}" class="cu-btn bg-olive margin-tb-sm lg" style="width:100%;" bindtap="__e">返回</button></block></view></view></view>