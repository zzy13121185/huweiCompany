
.home-img {
	width: 750rpx;
	height: 300rpx;
}
.home-img-image {
	width: 100%;
	height: 100%;
}
.home-middle {
	width: 700rpx;
	height: 190rpx;
	background-color: white;
	z-index: 999;
	margin-left: 25rpx;
	margin-top: 30rpx;
	border-radius: 10px;
	box-shadow: 0px 2px 12px rgba(0, 0, 0, 0.1);
}
.home-bottom {
	width: 700rpx;
	/* height: 190upx; */
	background-color: white;
	z-index: 999;
	margin-left: 25rpx;
	margin-top: 30rpx;
	border-radius: 10px;
	box-shadow: 0px 2px 12px rgba(0, 0, 0, 0.1);
}
.home-middle-box {
	width: 25%;
	height: 200rpx;
	float: left;
	text-align: center;
}
.home-middle-box-image {
	width: 32px;
	height: 32px;
	margin-top: 30rpx;
}
.home-middle-box-title {
	width: 100%;
	height: 60rpx;
	line-height: 60rpx;
	color: #555555;
	text-align: center;
	color: #8a8a8a;
}
.itemInfo {
	width: 700rpx;
	margin-left: 25rpx;
	height: auto;
	line-height: 100rpx;
	text-align: left;
	/* padding-left: 20upx; */
	box-sizing: border-box;
	color: #303133;
	background-color: #ffffff;
	border-radius: 10px;
	box-shadow: 0px 2px 12px rgba(0, 0, 0, 0.1);
	margin-top: 20rpx;
	float: left;
	padding: 20rpx;
}
.itemInfoTitle {
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
	box-sizing: border-box;
	padding-left: 20rpx;
}
.itemInfo-text {
	color: #f56c6c;
	font-size: 22px;
	/* font-weight: 700; */
	margin: 0 10rpx;
}
.smallBox {
	width: 600rpx;
	margin: 0 auto;
	height: auto;
}
.smallBoxItem {
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
	color: #99a9bf;
	border-bottom: 1px solid #dcdfe6;
	letter-spacing: 2rpx;
}
.smallBoxItemright {
	/* font-size: 22px; */
	color: #303133;
	float: right;
}
.smallBoxItemright-text {
	font-size: 22px;
	color: #f56c6c;
}

