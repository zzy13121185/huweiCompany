<view><block wx:if="{{qrCodeType=='XF'}}"><view style="margin-bottom:200rpx;"><block wx:for="{{xfData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view class="cu-form-group margin-top"><view class="title">{{item.Name}}</view></view><block wx:for="{{item.Detail}}" wx:for-item="site" wx:for-index="siteindex" wx:key="siteindex"><view class="cu-form-group" style="padding:0 40rpx;min-height:80rpx;"><view class="title" style="min-height:60rpx;height:auto;">{{site.content}}</view><switch class="{{[switchA?'checked':'']}}" checked="{{site.isCheck}}" data-info="{{site}}" data-title="{{item.Name}}" data-event-opts="{{[['change',[['SwitchA',['$event']]]]]}}" bindchange="__e"></switch></view></block></view></block><view class="cu-bar bg-white margin-top"><view class="action">图片上传</view><view class="action">{{''+imgrow.length+'/4'}}</view></view><view class="cu-form-group"><view class="grid col-4 grid-square flex-sub"><block wx:for="{{imgrow}}" wx:for-item="photo" wx:for-index="photoindex" wx:key="photoindex"><view class="bg-img" data-url="{{imgrow[photoindex]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" bindtap="__e"><image src="{{photo}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{photoindex}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view></view></block><block wx:if="{{imgrow.length<4}}"><view data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-cameraadd"></text></view></block></view></view><view class="cu-form-group margin-top"><view class="title">检查记录：</view><input placeholder="请输入检查记录" name="input" data-event-opts="{{[['input',[['__set_model',['','position','$event',[]]]]]]}}" value="{{position}}" bindinput="__e"/></view><view class="padding flex flex-direction margin-top"><button data-event-opts="{{[['tap',[['commitXf',['$event']]]]]}}" class="cu-btn bg-red margin-tb-sm lg" bindtap="__e">提交</button></view></view></block></view>