<view><cu-custom vue-id="1" bgColor="bg-olive" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">安全巡查</view></cu-custom><view style="display:flex;flex-direction:column;position:absolute;top:140rpx;bottom:0rpx;width:100%;"><view style="height:140rpx;"><view class="cu-form-group margin-top"><view class="title">选择校区</view><picker value="{{index}}" range="{{picker}}" data-event-opts="{{[['change',[['PickerChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(index>-1?picker[index]:'点击选择校区')+''}}</view></picker></view></view><view style="flex:1;overflow:scroll;"><view class="cu-timeline margin-top"><block wx:for="{{dataRow}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-item cur" data-info="{{item}}" data-event-opts="{{[['tap',[['showDetail',['$event']]]]]}}" bindtap="__e"><block wx:if="{{item.IsSafe=='True'&&item.IsCheck=='True'}}"><view class="content shadow-blur bg-green"><text>{{item.CheckTime}}</text>{{''+item.PlaceName+'(安全)'}}</view></block><block wx:if="{{item.IsSafe!='True'&&item.IsCheck=='True'}}"><view class="content shadow-blur bg-red"><text>{{item.CheckTime}}</text>{{''+item.PlaceName+'(存在隐患)'}}</view></block><block wx:if="{{item.IsCheck=='False'}}"><view class="content shadow-blur bg-orange"><text>{{item.CheckTime}}</text>{{''+item.PlaceName+'(暂未检查)'}}</view></block></view></block></view></view><view style="height:240rpx;"><view class="padding flex flex-direction margin-top"><button data-event-opts="{{[['tap',[['getData']]]]}}" class="cu-btn bg-red margin-tb-sm lg" bindtap="__e">查看</button></view></view></view></view>